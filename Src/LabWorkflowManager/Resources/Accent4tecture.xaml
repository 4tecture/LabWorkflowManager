<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Color x:Key="HighlightDarkColor">#FF009CCE</Color>
    <Color x:Key="HighlightLightColor">#FF009CCE</Color>
    <Color x:Key="AccentDarkColor">#FF009CCE</Color>
    <Color x:Key="AccentColor">#FF009CCE</Color>
    <Color x:Key="AccentColor2">#FF009CCE</Color>
    <Color x:Key="AccentColor3">#FF009CCE</Color>
    <Color x:Key="AccentColor4">#FF009CCE</Color>
    <LinearGradientBrush x:Key="BrandingBrush"
             EndPoint="0.001,0.5"
             StartPoint="1.002,0.5">
        <GradientStop Color="{DynamicResource HighlightDarkColor}" Offset="0"/>
        <GradientStop Color="#FF5FC316" Offset="1"/>
    </LinearGradientBrush>
    <SolidColorBrush x:Key="AccentColorBrush" Color="{StaticResource AccentColor}"/>
    <SolidColorBrush x:Key="AccentSelectedColorBrush" Color="White"/>
</ResourceDictionary>